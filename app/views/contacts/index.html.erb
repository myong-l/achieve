<h1>お問い合わせフォーム</h1>
<%= form_for(@contact) do |f| %>
<div class="explanation">
  <p><%= f.label :title %></p>
  <p><%= f.text_field :title %></p>
  <p><%= f.label :content %></p>
  <p><%= f.text_area :content %></p>
  <p><%= f.label :email %></p>
  <p><%= f.text_field :email %></p>
  <%= f.submit %>
<% end %>
<%= link_to 'Back', root_path %>
</div>
